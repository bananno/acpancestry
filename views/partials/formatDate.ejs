<%
    var monthNames = [
        'January', 'February', 'March', 'April',
        'May', 'June', 'July', 'August',
        'September', 'October', 'November', 'December'
    ];

    let dateString = '';

    if (params.date) {
        if (params.date.month && params.date.month > 0 && params.date.month <= 12) {
            dateString += monthNames[params.date.month - 1];
        }

        if (params.date.day) {
            if (dateString.length > 0) {
                dateString += ' ';
            }
            dateString += params.date.day;
        }

        if (params.date.year) {
            if (dateString.length > 0) {
                dateString += ', ';
            }
            dateString += params.date.year;
        }

        if (params.date.display) {
            dateString = params.date.display;
        }
    }
%>
<%= dateString %>
